#include <bits/stdc++.h>
using namespace std ;

int main()
{
  
    string s;
    cin>>s;

    // while :
    // for i : 0 -- size - 2 , s[i] == s[i + 1] ->
    while(true)
    {
        bool check = false;
        for(int i = 0 ; i < (int)s.size() - 1 ; i++)
        {
            if(s[i] == s[i + 1]) // i + 1 < (int)s.size()
            {
                check = true;
                s.erase(s.begin() + i , s.begin() + i + 2);
            }
        }
        if(!check)
            break;
    }
    cout<<s;
}
